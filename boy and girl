#include <iostream> 
using namespace std;
int main() { 
     int f=0,count=0; 
    char s[100]; 
    cout<<"enter the username";
    cin>>s;
    for(int i=0;s[i]!='\0';i++){ 
        
        for(int j=i+1;s[j]!='\0';j++){
            if(s[i]==s[j]){
                f=1; 
                break;
            }
        } 
        if(f==0){
            count++;
            f=0;
        }
    } 
    if(count%2==0)
    cout<<"chat with her"<<endl; 
    else{
    cout<<"ignore her"<<endl;}
    

    return 0;
}
