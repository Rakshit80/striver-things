#include <iostream> 
using namespace std;

int main()
{ 
    int el=0; 
    int count=0; 
    int arr[]={1,1,1,2,1,3,1,2};  
    int n=8;
      
     for(int i=0;i<n;i++){  
         if(count==0) 
         el=arr[i];
         if(arr[i]==el)
             count++; 
             else{count--;}
             
             }  
             int count1=0; 
             for(int i=0; i<n;i++) {
                  
                  if (el==arr[i])  
                  count1++;
             }
         if(count1>n/2) 
         cout<<el;
      }
     
